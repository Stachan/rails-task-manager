<div class="container">
  <h1 class="my-5">Edit #<%=@task.id %> - <%= @task.title %></h1>
    <%= form_with model: @task do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :details %>
      <%= f.text_field :details %>
      <%= f.submit %>
    <% end %>
</div>
